    "doi" : "10.7554/eLife.10627",
    "keywords" : [ "<i>Homo naledi</i>", "hominin", "Dinaledi Chamber", "paleoanthropology", "taphonomy" ],
    "versionDate" : "2015-09-10T00:00:00Z",
    "title" : "The many mysteries of <i>Homo naledi</i>",
    "volume" : 4,
    "status" : "vor",
    "id" : "10627",
    "statusDate" : "2015-09-10T00:00:00Z",
    "elocationId" : "e10627",
    "authorLine" : "Chris Stringer",
    "subjects" : [ {
